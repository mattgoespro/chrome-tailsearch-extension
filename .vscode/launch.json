{
  "configurations": [
    {
      "name": "debug",
      "type": "node",
      "request": "attach",
      "sourceMaps": true,
      "sourceMapRenames": true,
      "outFiles": ["${workspaceFolder}/dist"],
      "sourceMapPathOverrides": {
        "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
        "webpack://?:*/*": "${workspaceFolder}/*",
        "webpack://*/*": "${workspaceFolder}/*"
      },
      "preLaunchTask": "debug"
    }
  ]
}
